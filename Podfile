# Uncomment the next line to define a global platform for your project
platform :ios, "11.0"

target 'SDWebImage' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SDWebImage
  pod 'AanimationTransitionn', :git => 'https://github.com/geng199200/animatedtransitions.git'
  # pod 'ABLEManager', :git => 'https://github.com/ablesrls/ablemanager.git'
  pod 'ABTextFieldAnimations', :git => 'https://github.com/abhishaker17/abtextfieldanimations.git'
  # pod 'acceptSDK', :git => 'https://github.com/mpossvk/acceptsdk.git'
  pod 'ACKeyboardObserver', :git => 'https://github.com/yanke-guo/ackeyboardobserver.git'
  pod 'ActorKit', :git => 'https://github.com/tarbrain/actorkit.git'
  # pod 'AdaptiveTyping', :git => 'https://github.com/altece/adaptivetyping.git'

end

